exports[`uploader basic 1`] = `
<main>
  <mp-uploader
    id="uploader"
  >
    <wx-view
      class="weui-uploader "
    >
      <wx-view
        class="weui-uploader__hd"
      >
        <wx-view
          class="weui-uploader__overview"
        >
          <wx-view
            class="weui-uploader__title"
          >
            图片上传
          </wx-view>
          <wx-view
            class="weui-uploader__info"
          >
            0/5
          </wx-view>
        </wx-view>
        <wx-view
          class="weui-uploader__tips"
        >
          图片上传提示
        </wx-view>
      </wx-view>
      <wx-view
        class="weui-uploader__bd"
      >
        <wx-view
          class="weui-uploader__files"
        />
        <wx-view
          class="weui-uploader__input-box"
        >
          <wx-view
            class="weui-uploader__input"
            bind:tap="chooseImage"
          />
        </wx-view>
      </wx-view>
    </wx-view>
    <mp-gallery
      class="gallery"
      current=""
      hideOnClick="{{true}}"
      imgUrls="{{Array []}}"
      show="{{false}}"
      showDelete="{{true}}"
      bind:delete="deletePic"
    />
  </mp-uploader>
</main>
`;

